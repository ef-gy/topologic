{
  "name": "Topologic",
  "description": "Visualise basic geometric primitives and fractals in arbitrary-dimensional spaces.",
  "version": "11.0",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "topologic-16.png", "128": "topologic-128.png" },
  "sandbox": {
     "pages": ["topologic.html"]
  },
  "requirements": {
    "3D": {
      "features": ["webgl"]
    }
  },
  "offline_enabled": true
}
